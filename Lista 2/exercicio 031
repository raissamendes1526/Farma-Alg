program exercicio 031;

var saldo, credito: Integer;
begin
  read(saldo);
  
  if (saldo > 0) and (saldo <= 200) then 
  credito := 0
  else if (saldo > 200) and (saldo <= 400) then 
  credito := 20
  else if (saldo > 400) and (saldo <= 600) then 
  credito := 30
  else if (saldo > 600) then 
  credito := 40;
  
  writeln(saldo);
  
  if credito = 0 then 
  writeln(credito)
  else
  writeln(credito,'%');
end.
