program testa_se_primo;
var i: longint;

function eh_primo (num: longint): Boolean;
var divisor, cont, resto: Integer;

begin
  divisor := 2;
  cont := 0;

  while divisor < num do
    begin
  
      resto := num mod divisor;
  
      if resto = 0 then
      cont := cont + 1;
  
      divisor := divisor + 1;
    end;

  if cont = 0 then
    eh_primo := true
  else
    eh_primo := false;

end;

begin
    for i:= 2 to 10000 do
        if eh_primo (i) then
            writeln (i);
end.
