program calcula_percentual;
var antigo, atual: real;

function aumento_percentual (an, at: Real): Real;
var x: Real;
begin
  
  x := 100 * at / an;
  aumento_percentual := (x - 100) / 100;
end;

begin
    read (antigo, atual);
    while (antigo <> 0) or (atual <> 0) do
    begin
        writeln (aumento_percentual (antigo, atual):0:2);
        read (antigo, atual);
    end;
end.
