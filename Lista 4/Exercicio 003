program Exercicio 003
;

var n, q, cont: Integer;

begin
  
  read(n);
  read(q);
  
  cont := 0;
  
  while n <> 0 do 
    begin
      if q <> n * n then
      cont := cont + 1;
    
      read(n);
      
      if n = 0 then
        break
      else
        read(q);
    end;
  
  if cont > 0 then
    writeln('0')
  else
    writeln('1');
    
end.
